{
    "manifest_version": 3,
    "name": "Dobby Fact Checker",
    "version": "1.1",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://dobby-fact-checker-browser-extension.web3artisan.online/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "overlay.html",
                "overlay.css"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "icons": {
        "48": "dobby.png",
        "128": "dobby.png"
    }
}